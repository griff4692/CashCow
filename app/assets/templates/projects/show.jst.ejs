<% if(type==='highlight') { %>

  <%= model.escape('title') %>

  <a href="#discover">
    See More <%= catToHighlight %>
  </a>

  <%= model.escape('description') %>

  <label> <%= catToHighlight %>
    <%= model.escape(catToHighlight) %>
  </label>

<% } else { %>
  <a href="/#/projects/<%= model.id %>">
    <%= model.escape('title') %>
  </a>
<% } %>
